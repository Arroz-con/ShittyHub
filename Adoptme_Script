	Name = "Refesh player list",
	Callback = function()
		playerDropdown:Set(getPlayersInGame())
	end,
})

-- TradeTab:CreateToggle({
--     Name = "Semi-Auto Trade (manually choose items)",
--     CurrentValue = false,
--     Flag = "Toggle1",
--     Callback = function(Value)
--         getgenv().auto_trade_semi_auto = Value

--     end,
