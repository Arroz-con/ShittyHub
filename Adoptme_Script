    localPlayer.Idled:Connect(function()
        VirtualUser:ClickButton2(Vector2.new())
    end)
    task.spawn(function()
        while true do
            print('running frostclaw event')
            updateStatsGui()

            if Christmas2024.CreateAndStartLobby() then
                Christmas2024.StartGame()
            end

            task.wait(1)
        end
    end)
end
