	Name = "Select a player",
	Options = getPlayersInGame(),
	CurrentOption = { "" },
	MultipleOptions = false,
	Flag = "Dropdown1",
	Callback = function(Option)
		selectedPlayer = Option
		print(selectedPlayer)
	end,
})

