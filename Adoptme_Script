getgenv().AutoTradeToggle = TradeTab:CreateToggle({
	Name = "Auto Accept Trade",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(Value)
		getgenv().auto_accept_trade = Value
		while getgenv().auto_accept_trade do
			Trade:AutoAcceptTrade()
			ClickTradeWindowPopUps()
			task.wait(1)
		end
	end,
})

